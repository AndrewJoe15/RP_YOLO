<Window x:Class="RP_YOLO.Window_Main"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RP_YOLO"
        mc:Ignorable="d"
        Title="RPSoft" Height="450" Width="800">
    <Grid>
        <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center">            
            <Button x:Name="btn_YOLOV5_singleImage" Content="目标检测（图片）" Style="{DynamicResource MaterialDesignPaperDarkButton}"
                    Click="btn_YOLOV5_SingleImage_Click"/>
            <Button x:Name="btn_YOLOV5_camera" Content="目标检测（相机）" Style="{DynamicResource MaterialDesignPaperDarkButton}"
                    Click="btn_YOLOV5_camera_Click"/>
            <Button x:Name="btn_testPage" Content="测试页" Style="{DynamicResource MaterialDesignPaperDarkButton}"
                    Click="btn_testPage_Click"/>
        </StackPanel>
    </Grid>
</Window>
